{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "https://www.krakend.io/schema/e2e.json",
  "title": "E2E Testing Schema",
  "description": "Schema validation for the latest version of KrakenD End to End testing. Consider sticking to the specific version you are using",
  "examples": [
    {
      "$schema": "https://www.krakend.io/schema/v2.13/e2e.json",
      "@comment": "Makes sure that the debug endpoint returns a status ok",
      "in": {
        "method": "GET",
        "url": "http://localhost:8080/__debug/something",
        "header": {
          "User-Agent": "krakend-e2e-tool"
        }
      },
      "out": {
        "status_code": 200,
        "body": {
          "status": "ok"
        },
        "header": {
          "content-type": [ "application/json; charset=utf-8" ],
          "X-Krakend-Completed": [ "true" ],
          "Cache-Control": [ "" ]
        }
      }
    }
  ],
  "$ref": "v2.13/e2e.json"
}
